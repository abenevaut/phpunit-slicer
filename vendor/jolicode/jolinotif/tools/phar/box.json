{
    "base-path": "../../",
    "directories": [
        "bin",
        "src"
    ],
    "files": [
        "LICENSE",
        "vendor/autoload.php"
    ],
    "finder": [
        {
            "name": "*.php",
            "in": "vendor/composer"
        },
        {
            "name": "*.php",
            "exclude": ["Tests"],
            "in": "vendor/symfony/process"
        }
    ],
    "git-version": "package_version",
    "main": "jolinotif",
    "output": "tools/phar/build/jolinotif.phar"
}
